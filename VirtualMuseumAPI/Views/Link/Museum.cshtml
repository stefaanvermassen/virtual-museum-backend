@using VirtualMuseumAPI.Models
@model Museum
@{
    ViewBag.Title = "Museum";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script>
    // try to open the url
    window.open('virtualmuseum:/@Request.Url.PathAndQuery.ToString()');
</script>

<h2>Redirect page for @Model.Name</h2>
This museum was made by: @Model.AspNetUser.UserName<br/>
<a href="virtualmuseum:/@Request.Url.PathAndQuery.ToString()" class="btn btn-default btn-lg">Open museum</a>